# Bot Configuration
BOT_NAME=default-bot
NODE_ENV=development

# LLM API Settings
# For OpenRouter models (e.g., openrouter/mistralai/devstral-2512:free)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Browser Settings
HEADLESS=false
BROWSER_WIDTH=1920
BROWSER_HEIGHT=1080
BROWSER_TIMEOUT=30000

# Optional Browser Authentication (if needed)
# BROWSER_USERNAME=
# BROWSER_PASSWORD=

# Default Login Credentials (replace with your actual credentials)
# DEFAULT_USERNAME=
# DEFAULT_PASSWORD=

# Proxy Settings (if needed)
# PROXY_SERVER=
# PROXY_USERNAME=
# PROXY_PASSWORD=

# Logging
LOG_LEVEL=info
LOG_TO_FILE=false

# Site-Specific Configuration
# Example: Target website URLs and credentials
TARGET_URL=https://example.com/login
TARGET_USERNAME=your_username
TARGET_PASSWORD=your_password